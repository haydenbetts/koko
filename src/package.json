{
  "name": "vanilla",
  "version": "1.0.0",
  "description": "JavaScript example starter project",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:css && react-scripts start",
    "build": "npm run watch:css && CI=false react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "watch:css": "postcss src/assets/tailwind.css -o src/assets/main.css"
  },
  "dependencies": {
    "axios": "0.21.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.14.0",
    "react": "17.0.0",
    "react-dom": "17.0.0",
    "react-scripts": "3.4.3",
    "rxjs": "6.6.3"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "postcss-cli": "^8.2.0",
    "tailwindcss": "^1.9.6"
  },
  "keywords": [
    "javascript",
    "starter"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
